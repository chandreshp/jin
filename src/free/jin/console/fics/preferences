output-link.num-patterns = integer;9

#Matches "finger <playername>"
output-link.pattern-0 = string;(["'])([Ff][Ii][Nn][Gg][Ee][Rr] +[A-z]{3,17})\\1
output-link.command-0 = string;$2
output-link.index-0 = integer;2

#Matches "help <topic>"
output-link.pattern-1 = string;(["'])([Hh][Ee][Ll][Pp] +[-_0-9A-z\.]{1,})\\1
output-link.command-1 = string;$2
output-link.index-1 = integer;2

#Matches "ping <playername>"
output-link.pattern-2 = string;(["'])([Pp][Ii][Nn][Gg] +[A-z]{3,17})\\1
output-link.command-2 = string;$2
output-link.index-2 = integer;2

#Matches "match <playername>"
output-link.pattern-3 = string;(["'])([Mm][Aa][Tt][Cc][Hh] +[A-z]{3,17})\\1
output-link.command-3 = string;$2
output-link.index-3 = integer;2

#Matches "follow <playername>"
output-link.pattern-4 = string;(["'])([Ff][Oo][Ll][Ll][Oo][Ww] +[A-z]{3,17})\\1
output-link.command-4 = string;$2
output-link.index-4 = integer;2

#Matches "observe <playername or gamenumber>"
output-link.pattern-5 = string;(["'])([Oo][Bb]([Ss][Ee][Rr][Vv][Ee])? [0-9A-z]{1,15})\\1
output-link.command-5 = string;$2
output-link.index-5 = integer;2

#Matches "news <number>"
output-link.pattern-6 = string;(["'])([Nn][Ee][Ww][Ss] +[0-9]+)\\1
output-link.command-6 = string;$2
output-link.index-6 = integer;2

#Matches the "Type [next] to see next page." sent by the server when the text is longer than height
output-link.pattern-7 = string;^Type \\[(next)\\] to see next page\\.
output-link.command-7 = string;$1
output-link.index-7 = integer;1

#Matches "examine <playername, optional> <history or library number>"
output-link.pattern-8 = string;(["'])([Ee][Xx][Aa][Mm][Ii][Nn][Ee] +[0-9A-z]{1,15} +((%?[0-9]+)|[A-z]))\\1
output-link.command-8 = string;$2
output-link.index-8 = integer;2



